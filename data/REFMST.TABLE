	ENHRS DECIMAL(3, 1) NOT NULL DEFAULT 0 , 
	EPHRC DECIMAL(5, 1) NOT NULL DEFAULT 0 , 
	EPHRY DECIMAL(7, 1) NOT NULL DEFAULT 0 , 
	EPHRP DECIMAL(7, 1) NOT NULL DEFAULT 0 , 
	EPNRC DECIMAL(5, 1) NOT NULL DEFAULT 0 , 
	EPNRY DECIMAL(7, 1) NOT NULL DEFAULT 0 , 
	EPNRP DECIMAL(7, 1) NOT NULL DEFAULT 0 , 
	EHWRK DECIMAL(5, 1) NOT NULL DEFAULT 0 , 
	PRCDE CHAR(8) CCSID 37 NOT NULL DEFAULT '' , 
	PRDSC CHAR(50) CCSID 37 NOT NULL DEFAULT '' , 
	PRRSP CHAR(30) CCSID 37 NOT NULL DEFAULT '' , 
	PRSTR DECIMAL(6, 0) NOT NULL DEFAULT 0 , 
	PREND DECIMAL(6, 0) NOT NULL DEFAULT 0 , 
	PRCMP DECIMAL(6, 0) NOT NULL DEFAULT 0 , 
	PREST DECIMAL(9, 1) NOT NULL DEFAULT 0 , 
	PRHRC DECIMAL(7, 1) NOT NULL DEFAULT 0 , 
	PRHRY DECIMAL(9, 1) NOT NULL DEFAULT 0 , 
	PRHRP DECIMAL(9, 1) NOT NULL DEFAULT 0 , 
	RSCDE CHAR(8) CCSID 37 NOT NULL DEFAULT '' , 
	RSDSC CHAR(50) CCSID 37 NOT NULL DEFAULT '' , 
	RSHRC DECIMAL(7, 1) NOT NULL DEFAULT 0 , 
	RSHRY DECIMAL(9, 1) NOT NULL DEFAULT 0 , 
	RSHRP DECIMAL(9, 1) NOT NULL DEFAULT 0 , 
	CTCDE CHAR(6) CCSID 37 NOT NULL DEFAULT '' , 
	CWKDT NUMERIC(6, 0) NOT NULL DEFAULT 0 , 
	CMTDT NUMERIC(6, 0) NOT NULL DEFAULT 0 , 
	CALLE CHAR(1) CCSID 37 NOT NULL DEFAULT '' , 
	ACDAT NUMERIC(6, 0) NOT NULL DEFAULT 0 , 
	TFRRN DECIMAL(5, 0) NOT NULL DEFAULT 0 )   
	  
	RCDFMT RCREF      ; 
  
LABEL ON COLUMN REFMST 
( DATFL IS 'DATE                MMDDYY' , 
	EMPNO IS 'EMPLOYEE            NUMBER' , 
	ENAME IS 'EMPLOYEE            NAME' , 
	EMCAT IS 'EMP                 CAT' , 
	EDEPT IS 'EMPL                DEPT' , 
