	DATFL DECIMAL(6, 0) NOT NULL DEFAULT 0 , 
	EMPNO DECIMAL(6, 0) NOT NULL DEFAULT 0 , 
	ENAME CHAR(30) CCSID 37 NOT NULL DEFAULT '' , 
	EMCAT CHAR(1) CCSID 37 NOT NULL DEFAULT '' , 
	EDEPT CHAR(5) CCSID 37 NOT NULL DEFAULT '' , 
