	ACDAT TEXT IS 'ACTUAL DATE WORKED' , 
	TFRRN TEXT IS 'TRANSACTION FILE RRN' ) ; 
  
GRANT DELETE , INSERT , SELECT , UPDATE   
ON REFMST TO PUBLIC ; 
  
