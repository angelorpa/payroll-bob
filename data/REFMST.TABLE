	ACDAT IS 'ACTUAL DATE         WORKED' , 
	TFRRN IS 'RELATIVE            RECORD NO' ) ; 
  
LABEL ON COLUMN REFMST 
( ACREC TEXT IS 'ACTIVE RECORD CODE' , 
	DATFL TEXT IS 'DATE FIELD' , 
	EMPNO TEXT IS 'EMPLOYEE NUMBER' , 
	ENAME TEXT IS 'EMPLOYEE NAME' , 
	EMCAT TEXT IS 'EMPLOYEE CATEGORY' , 
	EDEPT TEXT IS 'EMPLOYEE DEPARTMENT' , 
	ELOCN TEXT IS 'EMPLOYEE LOCATION' , 
	EUSRI TEXT IS 'EMPLOYEE USRID' , 
	ENHRS TEXT IS 'EMPLOYEE NORMAL WEEK HOURS' , 
	EPHRC TEXT IS 'PROJECT HOURS CURRENT MONTH' , 
	EPHRY TEXT IS 'PROJECT HOURS YEAR TO DATE' , 
	EPHRP TEXT IS 'PROJECT HOURS PRIOR YEAR' , 
	EPNRC TEXT IS 'NON PROJECT HOURS CURR MONTH' , 
	EPNRY TEXT IS 'NON PROJECT HOURS YTD' , 
	EPNRP TEXT IS 'NON PROJECT HOURS PRIOR YEAR' , 
	EHWRK TEXT IS 'EMPLOYEE HOURS WORKED' , 
	PRCDE TEXT IS 'PROJECT CODE' , 
	PRDSC TEXT IS 'PROJECT DESCRIPTION' , 
